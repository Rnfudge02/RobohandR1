{
  "configurations": [
    {
      "name": "Pico",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/Include/**",
        "${workspaceFolder}/Dependencies/pico-sdk/**",
        //"${workspaceFolder}/Dependencies/pico-sdk/src/rp2_common/pico_platform/include",
        "${workspaceFolder}/Dependencies/pico-sdk/src/rp2350/pico_platform/include",
        "${workspaceFolder}/Dependencies/pico-sdk/src/common/pico_base_headers/include",
        "${workspaceFolder}/Dependencies/bmm350-sensorapi",
        "${workspaceFolder}/Dependencies/bmp390-sensorapi"
      ],
      "defines": [
        "PICO_BOARD=\"pico2\"",
        "PICO_PLATFORM=\"rp2350-arm-s\"",
        "PICO_RAM_FUNCTION_TRACK=1",
        "PICO_XIP_CACHE_ENABLED=1",
        "PICO_FPU_ENABLED=1"
      ],
      "compilerPath": "${env:HOME}/.pico-sdk/toolchain/14_2_Rel1/bin/arm-none-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-arm"
    }
  ],
  "version": 4
}